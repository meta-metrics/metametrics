{
    "optimizer_name": "xgb",
    "optimizer_args": {
        "xgb_type": "regressor",
        "objective_fn": "kendall",
        "hpm_search_mode": "bayes_search",
        "cv_split": 10,
        "bayes_n_iter": 25,
        "param_space": {
            "n_estimators": {
                "type": "Categorical",
                "low": 100,
                "high": 1500,
                "step": 100,
                "prior": "uniform"
            },
            "eta": {
                "type": "Real",
                "low": 0.01,
                "high": 0.1,
                "prior": "log-uniform"
            }
        },
        "fixed_param": {
            "min_child_weight": 3,
            "max_depth": 5,
            "gamma": 0.1,
            "subsample": 0.95,
            "colsample_bytree": 0.95,
            "reg_alpha": 0.1,
            "reg_lambda": 0.1
        }
    }
}